{
  "name": "@iannuttall/dotagents",
  "version": "0.1.1",
  "description": "Canonical .agents manager with symlinks for popular AI tools",
  "type": "module",
  "license": "MIT",
  "bin": {
    "dotagents": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/cli.tsx",
    "build": "bun build src/cli.tsx --outdir dist --target node --format esm --external react --external ink --external ink-select-input --external ink-text-input --external ink-spinner",
    "type-check": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "ink": "^5.0.1",
    "ink-select-input": "^6.1.0",
    "ink-text-input": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
